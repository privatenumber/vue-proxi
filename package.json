{
  "name": "vue-proxi",
  "version": "1.0.0",
  "description": "Tiny proxy component for Vue.js",
  "keywords": [
    "vue",
    "component",
    "proxi",
    "parent-child",
    "proxy",
    "provide",
    "inject",
    "reactive"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/proxi.cjs.js",
  "module": "dist/proxi.esm.js",
  "unpkg": "dist/proxi.js",
  "scripts": {
    "prepare": "simple-git-hooks",
    "build": "rollup -c --environment NODE_ENV:production",
    "dev-build": "rollup -cw",
    "dev": "jest --watchAll",
    "test": "jest",
    "lint": "eslint --ext js,vue ."
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint",
      "pnpm test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/privatenumber/vue-proxi.git"
  },
  "author": "Hiroki Osame <hiroki.osame@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/privatenumber/vue-proxi/issues"
  },
  "homepage": "https://github.com/privatenumber/vue-proxi#readme",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.2",
    "@vue/test-utils": "^1.0.3",
    "babel-jest": "^26.0.1",
    "core-js": "^3.30.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "rollup": "^3.22.0",
    "rollup-plugin-filesize": "^10.0.0",
    "simple-git-hooks": "^2.8.1",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  }
}
