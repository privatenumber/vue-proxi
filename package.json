{
  "name": "vue-proxi",
  "version": "1.0.0-beta.1",
  "description": "Tiny proxy component for Vue.js",
  "keywords": [
    "vue",
    "component",
    "proxi",
    "parent-child",
    "proxy",
    "provide",
    "inject",
    "reactive"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/proxi.cjs.js",
  "module": "dist/proxi.esm.js",
  "unpkg": "dist/proxi.js",
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:production",
    "dev-build": "rollup -cw",
    "dev": "jest --watchAll",
